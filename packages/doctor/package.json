{
  "name": "@iceworks/doctor",
  "description": "Analyse react/rax projects, troubleshooting and automatically fixing errors",
  "version": "0.2.3",
  "keywords": [
    "doctor",
    "analysis",
    "lint",
    "troubleshooting"
  ],
  "bin": {
    "iceworks-doctor": "bin/iceworks-doctor"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "compile": "rm -rf lib && tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run compile"
  },
  "dependencies": {
    "@babel/parser": "^7.12.5",
    "@babel/traverse": "^7.12.5",
    "@iceworks/spec": "^1.0.2",
    "@jscpd/core": "^3.3.3",
    "argv-parse": "^1.0.1",
    "eslint": "^6.0.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "ignore": "^5.1.8",
    "jscpd": "^3.3.3",
    "stylelint": "^13.7.0",
    "typescript": "^3.0.0",
    "typhonjs-escomplex": "^0.1.0"
  }
}
