// src/node/index.ts
<% packages.forEach(function({ activateFunc, deactivateFunc, packageName }){-%>
import { activate as <%=activateFunc%>, deactivate as <%=deactivateFunc%> } from '<%=packageName%>';
<%}) -%>

export default class KitNode {
  activate(context) {
  <% packages.forEach(function({ activateFunc }){-%>
    <%=activateFunc%>(context);
  <%}) -%>
  }

  deactivate() {
  <% packages.forEach(function({ deactivateFunc }){-%>
    <%=deactivateFunc%>();
  <%}) -%>
  }
}
