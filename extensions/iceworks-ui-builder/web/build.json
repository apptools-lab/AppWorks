{
  "devServer": {
    "writeToDisk": true
  },
  "mpa": true,
  "publicPath": "./",
  "outputDir": "../build",
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": [
          {
            "name": "@alife/dpl-iceluna",
            "default": true
          }
        ]
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn",
          "en-au"
        ]
      }
    ],
    [
      "build-plugin-css-assets-local",
      {
        "activeInDev": true
      }
    ],
    "./build.plugin.js"
  ]
}