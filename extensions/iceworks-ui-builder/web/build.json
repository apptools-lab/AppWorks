{
  "devServer": {
    "writeToDisk": true
  },
  "mpa": true,
  "publicPath": "./",
  "outputDir": "../build",
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": [
          {
            "name": "@alifd/theme-iceworks-dark",
            "default": true
          },
          {
            "name": "@alife/dpl-iceluna"
          }
        ]
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": [
          "zh-cn"
        ]
      }
    ],
    [
      "build-plugin-css-assets-local",
      {
        "activeInDev": true
      }
    ],
    "./build.plugin.js"
  ]
}