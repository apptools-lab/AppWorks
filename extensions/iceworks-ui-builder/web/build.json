{
  "devServer": {
    "writeToDisk": true
  },
  "mpa": true,
  "vendor": false,
  "externals": {
    "fetch-jsonp": "FetchJsonp",
    "jsonuri": "Jsonuri",
    "js-beautify": "JsBeautify",
    "react-monaco-editor": "ReactMonacoEditor"
  },
  "publicPath": "./",
  "outputDir": "../build",
  "plugins": [
    [
      "build-plugin-fusion",
      {
        "themePackage": [
          {
            "name": "@alife/dpl-iceluna",
            "default": true
          },
          {
            "name": "@alifd/theme-iceworks-dark"
          }
        ]
      }
    ],
    [
      "build-plugin-moment-locales",
      {
        "locales": ["zh-cn", "en-au"]
      }
    ],
    [
      "build-plugin-css-assets-local",
      {
        "activeInDev": true
      }
    ],
    "./build.plugin.js"
  ]
}
