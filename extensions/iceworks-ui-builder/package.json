{
  "name": "iceworks-ui-builder",
  "displayName": "UI Designer",
  "description": "Build UI by low-code way",
  "publisher": "iceworks-team",
  "version": "0.1.20",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ICE",
    "Rax",
    "React",
    "Low-code",
    "visual",
    "iceworks"
  ],
  "icon": "assets/logo.png",
  "activationEvents": [
    "onCommand:iceworks-ui-builder.generate-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/iceworks.git"
  },
  "main": "./build/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "iceworks-ui-builder.generate-component",
        "title": "%iceworksUIBuilder.commands.generateComponent.title%"
      }
    ],
    "configuration": {
      "title": "Iceworks UI Builder",
      "properties": {
        "iceworks.generateComponentPath": {
          "type": "string",
          "default": "",
          "description": "%iceworksUIBuilder.configuration.properties.iceworks.generateComponentPath.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "rm -rf build && webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "compile": "tsc -p ./tsconfig.json"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.160",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.41.0",
    "ts-loader": "^7.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@iceworks/i18n": "^0.1.0",
    "@iceworks/material-engine": "^0.1.0",
    "@iceworks/recorder": "^0.1.0",
    "@iceworks/vscode-webview": "^0.1.1"
  },
  "homepage": "https://github.com/ice-lab/iceworks/blob/master/extensions/iceworks-ui-builder/README.md",
  "bugs": {
    "url": "https://github.com/ice-lab/iceworks/issues",
    "email": "iceworksteam@163.com"
  }
}
