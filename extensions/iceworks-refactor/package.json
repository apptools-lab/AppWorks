{
  "name": "iceworks-refactor",
  "displayName": "React Refactor",
  "description": "Easily refactor Component in React/Rax.",
  "publisher": "iceworks-team",
  "icon": "assets/logo.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "iceworks",
    "React",
    "refactor"
  ],
  "activationEvents": [
    "onCommand:iceworks-refactor.component.dir-and-reference.remove",
    "onCommand:iceworks-refactor.component.selection-and-reference.remove"
  ],
  "contributes": {
    "commands": [
      {
        "command": "iceworks-refactor.component.dir-and-reference.remove",
        "title": "%iceworks-refactor.component.dir-and-reference.remove.title%"
      },
      {
        "command": "iceworks-refactor.component.selection-and-reference.remove",
        "title": "%iceworks-refactor.component.selection-and-reference.remove.title%"
      }
    ],
    "keybindings": [
      {
        "command": "iceworks-refactor.component.selection-and-reference.remove",
        "key": "shift+d",
        "when": "iceworks:isJSXFile"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "iceworks-refactor.component.dir-and-reference.remove",
          "group": "iceworks-refactor",
          "when": "explorerResourceIsFolder"
        }
      ],
      "editor/context": [
        {
          "command": "iceworks-refactor.component.selection-and-reference.remove",
          "group": "iceworks-refactor",
          "when": "iceworks:isJSXFile"
        }
      ]
    }
  },
  "main": "./build/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/iceworks.git"
  },
  "bugs": {
    "url": "https://github.com/ice-lab/iceworks/issues",
    "email": "iceworksteam@163.com"
  },
  "scripts": {
    "vscode:prepublish": "rm -rf build && npm run build:extension",
    "build:extension": "webpack --mode production",
    "watch:extension": "webpack --mode development --watch --info-verbosity verbose",
    "compile": "tsc -p ./",
    "watch": "tsc --watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./build/__tests__/runTest.js"
  },
  "dependencies": {
    "@babel/generator": "^7.13.9",
    "@babel/parser": "^7.13.11",
    "@babel/traverse": "^7.13.0",
    "@babel/types": "^7.13.0",
    "@iceworks/common-service": "^0.2.0",
    "@iceworks/project-service": "^0.2.0",
    "@iceworks/project-utils": "^0.3.3",
    "fs-extra": "^9.1.0",
    "fs-readdir-recursive": "^1.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "decache": "^4.6.0",
    "glob": "^7.1.6",
    "istanbul": "^0.4.5",
    "mocha": "^8.3.2",
    "remap-istanbul": "^0.13.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.5.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
