{
  "name": "iceworks-refactor",
  "displayName": "",
  "description": "",
  "publisher": "iceworks-team",
  "icon": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "iceworks",
    "React",
    "iceworks-refactor"
  ],
  "activationEvents": [
    "onCommand:iceworks-refactor.delete"
  ],
  "contributes": {
    "commands": [
      {
        "command": "iceworks-refactor.delete",
        "title": "%iceworks-refactor.delete.title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "iceworks-refactor.delete",
          "group": "iceworks",
          "when": "iceworks:isJSXFile"
        }
      ]
    }
  },
  "main": "./build/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/iceworks.git"
  },
  "bugs": {
    "url": "https://github.com/ice-lab/iceworks/issues",
    "email": "iceworksteam@163.com"
  },
  "scripts": {
    "vscode:prepublish": "rm -rf build && npm run build:web && webpack --mode production",
    "build:extension": "webpack --mode production",
    "watch:extension": "webpack --mode development --watch --info-verbosity verbose",
    "compile": "tsc -p ./",
    "watch": "tsc --watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./build/test/runTest.js"
  },
  "dependencies": {
    "@babel/generator": "^7.13.9",
    "@babel/parser": "^7.13.11",
    "@babel/traverse": "^7.13.0",
    "@babel/types": "^7.13.0",
    "@iceworks/common-service": "^0.2.0",
    "@iceworks/project-service": "^0.2.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.6",
    "mocha": "^8.3.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "vscode-test": "^1.5.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
