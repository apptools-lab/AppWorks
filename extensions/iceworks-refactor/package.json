{
  "name": "iceworks-refactor",
  "displayName": "",
  "description": "",
  "publisher": "iceworks-team",
  "icon": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "iceworks",
    "React",
    "iceworks-refactor"
  ],
  "activationEvents": [
    "onCommand:iceworks-refactor.hello"
  ],
  "main": "./build/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/iceworks.git"
  },
  "bugs": {
    "url": "https://github.com/ice-lab/iceworks/issues",
    "email": "iceworksteam@163.com"
  },
  "scripts": {
    "vscode:prepublish": "rm -rf build && npm run build:web && webpack --mode production",
    "build:extension": "webpack --mode production",
    "watch:extension": "webpack --mode development --watch --info-verbosity verbose",
    "compile": "tsc -p ./tsconfig.json"
  },
  "contributes": {
    "commands": [
      {
        "command": "iceworks-refactor.hello",
        "title": "%commands.hello.title%"
      }
    ]
  },
  "dependencies": {
    "@iceworks/common-service": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
