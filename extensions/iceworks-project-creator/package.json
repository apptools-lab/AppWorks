{
  "name": "iceworks-project-creator",
  "displayName": "iceworks Project Creator",
  "description": "Quick to create a new react app.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "./public/logo.png",
  "activationEvents": [
    "onCommand:projectCreator.start"
  ],
  "main": "./out/src-extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "projectCreator.start",
        "title": "Project Creator"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/iceworks.git"
  },
  "scripts": {
    "start": "icejs start",
    "build": "icejs build",
    "vscode:prepublish": "rm -rf out && npm run compile && npm run build",
    "compile": "tsc -p tsconfig.extension.json",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p tsconfig.extension.json",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.35",
    "@types/vscode": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "build-plugin-css-assets-local": "^0.1.0",
    "build-plugin-fusion": "^0.1.3",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "ice.js": "^1.0.0",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@alifd/next": "^1.19.4",
    "@alifd/theme-design-pro": "^0.x",
    "ejs": "^3.0.1",
    "fs-extra": "^9.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "vscode": "^1.1.37"
  }
}
