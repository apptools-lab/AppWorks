{
	"name": "iceworks-material-helper",
	"displayName": "Iceworks Material Helper",
	"description": "Easily use materials in iceworks.",
	"publisher": "iceworks-team",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"icon": "assets/logo.png",
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ice-lab/iceworks.git"
	},
	"main": "./build/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "rm -rf build && npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./tsconfig.json"
	},
	"devDependencies": {
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"typescript": "^3.6.4"
	}
}
