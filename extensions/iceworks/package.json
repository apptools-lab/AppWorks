      {
        "command": "iceworks-material-helper.showMaterialDocsForCurrentFile",
        "title": "%iceworksMaterialHelper.commands.showDocumentMaterialQuickPicks.title%"
      },
      {
        "command": "iceworks-doctor.dashboard",
        "title": "%iceworksDoctor.commands.dashboard.title%"
      },
      {
        "command": "iceworks-doctor.scan",
        "title": "%iceworksDoctor.commands.scan.title%"
      },
      {
        "command": "core.def.createIterate.open",
        "title": "打开新建迭代面板"
      },
      {
        "command": "core.def.createIterate.close",
        "title": "关闭新建迭代面板"
      },
      {
        "command": "core.def.switch.iteration",
        "title": "快速切换迭代"
      },
      {
        "command": "core.def.localConfig.get",
        "title": "def local config"
      },
      {
        "command": "core.def.refresh.information",
        "title": "刷新应用信息",
        "icon": {
          "dark": "icons/dark/refresh.svg",
          "light": "icons/light/refresh.svg"
        }
      },
      {
        "command": "core.def.forceRefresh.information",
        "title": "选择远程数据请求地址"
      },
      {
        "command": "core.def.view.application",
        "title": "查看应用详细信息"
      }
    ],
    "jsonValidation": [
      {
        "fileMatch": "build.json",
        "url": "./schemas/ice.build.en.json"
      },
      {
        "fileMatch": "app.json",
        "url": "./schemas/rax.app.en.json"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "iceworksApp.recorder.recordCompletionItemSelect",
          "when": "false"
        },
        {
          "command": "iceworksApp.npmScripts.run",
          "when": "false"
        },
        {
          "command": "iceworksApp.npmScripts.stop",
          "when": "false"
        },
        {
          "command": "iceworksApp.pages.add",
          "when": "false"
        },
        {
          "command": "iceworksApp.pages.refresh",
          "when": "false"
        },
        {
          "command": "iceworksApp.pages.openFile",
          "when": "false"
        },
        {
          "command": "iceworksApp.pages.delete",
          "when": "false"
        },
        {
          "command": "iceworksApp.components.add",
          "when": "false"
        },
        {
          "command": "iceworksApp.components.refresh",
          "when": "false"
        },
        {
          "command": "iceworksApp.components.delete",
          "when": "false"
        },
        {
          "command": "iceworksApp.components.openFile",
          "when": "false"
        },
        {
          "command": "iceworksApp.nodeDependencies.refresh",
          "when": "false"
        },
        {
          "command": "iceworksApp.nodeDependencies.upgrade",
          "when": "false"
        }
      ],
      "editor/context": [
        {
          "command": "iceworks-material-helper.material-importer.start",
          "when": "editorTextFocus && iceworks:isJSXFile",
          "group": "iceworksMaterialHelper"
        },
        {
          "command": "iceworks-material-helper.showMaterialDocsForCurrentFile",
          "group": "iceworksMaterialHelper",
          "when": "editorTextFocus && iceworks:isJSXFile"
        }
      ],
      "editor/title": [
        {
          "command": "iceworksApp.editorMenu.runDebug",
          "when": "iceworks:showScriptIconInEditorTitleMenu",
          "group": "navigation@01"
        },
        {
          "command": "iceworksApp.editorMenu.runBuild",
          "when": "iceworks:showScriptIconInEditorTitleMenu",
          "group": "navigation@02"
        },
        {
          "command": "iceworksApp.editorMenu.DefPublish",
          "group": "navigation@03",
          "when": "iceworks:showScriptIconInEditorTitleMenu && iceworks:isAliInternal"
        }
      ],
      "explorer/context": [
        {
          "command": "iceworks-config-helper.configPanel.start",
          "when": "resourceExtname == .json",
          "group": "2_workspace"
        }
      ],
      "view/title": [
        {
          "command": "iceworksApp.npmScripts.refresh",
          "when": "view == npmScripts",
          "group": "navigation"
        },
        {
          "command": "iceworksApp.pages.add",
          "when": "view == pages",
          "group": "navigation"
        },
        {
          "command": "iceworksApp.pages.refresh",
          "when": "view == pages",
          "group": "navigation"
        },
        {
          "command": "iceworksApp.components.add",
          "when": "view == components",
          "group": "navigation"
        },
        {
          "command": "iceworksApp.components.refresh",
          "when": "view == components",
          "group": "navigation"
        },
        {
          "command": "iceworksApp.nodeDependencies.addDepsAndDevDeps",
          "when": "view == nodeDependencies",
          "group": "navigation@11"
        },
        {
          "command": "iceworksApp.nodeDependencies.reinstall",
          "when": "view == nodeDependencies",
          "group": "navigation@12"
        },
        {
          "command": "iceworksApp.nodeDependencies.refresh",
          "when": "view == nodeDependencies",
          "group": "navigation@99"
        },
        {
          "command": "core.def.refresh.information",
          "when": "view == PubInfo",
          "group": "navigation"
        },
        {
          "command": "core.def.view.application",
          "when": "view == PubInfo"
        },
        {
          "command": "core.def.forceRefresh.information",
          "when": "view == PubInfo"
        }
      ],
      "view/item/context": [
        {
          "command": "iceworksApp.quickEntries.start",
          "when": "view == quickEntries && viewItem == quickEntry",
          "group": "inline"
        },
        {
          "command": "iceworksApp.npmScripts.run",
          "when": "view == npmScripts && viewItem == script",
          "group": "inline"
        },
        {
          "command": "iceworksApp.npmScripts.stop",
          "when": "view == npmScripts && viewItem == script",
          "group": "inline"
        },
        {
          "command": "iceworksApp.pages.delete",
          "when": "view == pages && viewItem == page",
          "group": "inline"
        },
        {
          "command": "iceworksApp.components.delete",
          "when": "view == components && viewItem == component",
          "group": "inline"
        },
        {
          "command": "iceworksApp.nodeDependencies.upgrade",
          "when": "view == nodeDependencies && viewItem == outdatedDependency",
          "group": "inline"
        },
        {
          "command": "iceworksApp.nodeDependencies.dependencies.add",
          "when": "view == nodeDependencies && viewItem == dependencies",
          "group": "inline"
        },
        {
          "command": "iceworksApp.nodeDependencies.devDependencies.add",
          "when": "view == nodeDependencies && viewItem == devDependencies",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "iceworksApp",
          "title": "Iceworks",
          "icon": "assets/sidebar-logo.png"
        }
      ]
    },
    "views": {
      "iceworksApp": [
        {
          "id": "welcome",
          "name": "Welcome",
          "when": "iceworks:projectIsNotTarget"
        },
        {
          "id": "quickEntries",
          "name": "%iceworksApp.view.quickEntries.name%"
        },
        {
          "id": "npmScripts",
          "name": "%iceworksApp.view.npmScripts.name%",
          "when": "!iceworks:projectIsNotTarget"
        },
        {
          "id": "pages",
          "name": "%iceworksApp.view.pages.name%",
          "when": "!iceworks:projectIsNotTarget && !iceworks:projectIsPegasus"
        },
        {
          "id": "components",
          "name": "%iceworksApp.view.components.name%",
          "when": "!iceworks:projectIsNotTarget && !iceworks:projectIsPegasus"
        },
        {
          "id": "nodeDependencies",
          "name": "%iceworksApp.view.nodeDependencies.name%",
          "when": "!iceworks:projectIsNotTarget"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "welcome",
        "contents": "%iceworksApp.viewsWelcome.welcome.contents%"
      },
      {
        "view": "quickEntries",
        "contents": ""
      },
      {
        "view": "npmScripts",
        "contents": "%iceworksApp.viewsWelcome.npmScripts.contents%"
      },
      {
        "view": "pages",
        "contents": "%iceworksApp.viewsWelcome.pages.contents%"
      },
      {
        "view": "components",
        "contents": "%iceworksApp.viewsWelcome.components.contents%"
      },
      {
        "view": "nodeDependencies",
        "contents": "%iceworksApp.viewsWelcome.nodeDependencies.contents%"
      }
    ],
    "configuration": {
      "title": "Iceworks",
      "properties": {
        "iceworks.packageManager": {
          "type": "string",
          "default": "npm",
          "enum": [
            "npm",
            "cnpm",
            "tnpm",
            "yarn"
          ],
          "description": "%iceworksMaterialHelper.configuration.properties.iceworks.packageManager.description%"
        },
        "iceworks.npmRegistry": {
          "type": "string",
          "default": "https://registry.npm.taobao.org",
          "enum": [
            "https://registry.npmjs.org",
            "https://registry.npm.taobao.org"
          ],
          "description": "%iceworksMaterialHelper.configuration.properties.iceworks.npmRegistry.description%"
        },
        "iceworks.showWelcomePage": {
          "type": "boolean",
          "default": true,
          "description": "%iceworksApp.configuration.properties.iceworks.showWelcomePage.description%"
        },
        "iceworks.materialSources": {
          "type": "array",
          "default": [],
          "description": "%iceworksMaterialHelper.configuration.properties.iceworks.materialSources.description%",
          "items": {
            "type": "object",
            "description": "Source",
            "properties": {
              "name": {
                "type": "string",
                "description": "name"
              },
              "type": {
                "type": "string",
                "description": "react or rax"
              },
              "source": {
                "type": "string",
                "description": "URL",
                "format": "URL"
              },
              "description": {
                "type": "string",
                "description": "description"
              }
            }
          }
        },
        "iceworks.workspace": {
          "type": "string",
          "default": "",
          "description": "Iceworks workspace"
        },
        "iceworks.user": {
          "type": "object",
          "default": {
            "empId": "",
            "account": "",
            "gitlabToken": ""
          },
          "properties": {
            "empId": {
              "type": "string",
              "description": "工号"
            },
            "account": {
              "type": "string",
              "description": "域账号"
            },
            "gitlabToken": {
              "type": "string",
              "description": "gitlab 私人 token"
            }
          },
          "description": "Iceworks user information"
        },
        "Iceworks.Doctor.enableCheckSecurityPracticesOnSave": {
          "scope": "window",
          "type": "boolean",
          "default": true,
          "description": "Check security practices when file saved."
        },
        "iceworks.materialHelper.openDocLinkInsideVSCode": {
          "type": "boolean",
          "default": false,
          "description": "%iceworksMaterialHelper.configuration.openInBrowserDescription%",
          "title": "Open Doc Link Inside VSCode"
        },
        "iceworks.generatePagePath": {
          "type": "string",
          "default": "",
          "description": "%iceworksMaterialHelper.configuration.properties.iceworks.generatePagePath.description%"
        },
        "iceworks.generateComponentPath": {
          "type": "string",
          "default": "",
          "description": "%iceworksMaterialHelper.configuration.properties.iceworks.generateComponentPath.description%"
        }
      },
      "type": "object"
    }
  },
  "activationEvents": [
    "onCommand:iceworks-config-helper.configPanel.start",
    "onLanguage:json",
    "onCommand:iceworks-project-creator.start",
    "onStartupFinished",
    "*",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:css",
    "onLanguage:less",
    "onLanguage:sass",
    "onLanguage:scss",
    "onCommand:iceworks-ui-builder.design-component",
    "onCommand:iceworks-material-helper.material-importer.start",
    "onCommand:iceworks-material-helper.component-creator.start",
    "onCommand:iceworks-material-helper.page-generator.start",
    "onCommand:iceworks-material-helper.page-creator.start",
    "onCommand:iceworks-doctor.scan",
    "onCommand:iceworks-doctor.dashboard"
  ],
  "dependencies": {
    "@ali/ide-extensions-iceworks-app": "*",
    "@ali/ide-extensions-iceworks-config-helper": "*",
    "@ali/ide-extensions-iceworks-doctor": "*",
    "@ali/ide-extensions-iceworks-material-helper": "*",
    "@ali/ide-extensions-iceworks-project-creator": "*",
    "@ali/ide-extensions-iceworks-style-helper": "*",
    "@ali/ide-extensions-iceworks-ui-builder": "*",
    "@ali/kit-runner": "^0.1.0",
    "@ali/publish-visual": "^0.2.24"
  },
  "devDependencies": {
    "vscode": "^1.1.28",
    "@ali/kaitian": "latest"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "NODE_ENV=development kit-builder compile",
    "watch": "NODE_ENV=development kit-builder watch"
  },
  "kaitianContributes": {
    "nodeMain": "./out/node/index.js",
    "browserMain": "./out/browser/index.js",
    "viewsProxies": [
      "PubInfo",
      "CreateIteration"
    ],
    "browserViews": {
      "right": {
        "type": "add",
        "view": [
          {
            "id": "PubInfo",
            "iconPath": "icons/logo.svg",
            "title": "基础信息"
          }
        ]
      },
      "editor": {
        "type": "add",
        "view": [
          {
            "id": "CreateIteration",
            "scheme": "def-create-iteration",
            "tabTitle": "创建迭代",
            "tabIconPath": "icons/iteration.svg",
            "renderMode": 1
          }
        ]
      }
    }
  }
}
