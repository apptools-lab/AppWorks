{
  "name": "iceworks",
  "displayName": "Iceworks",
  "description": "Visual Intelligent Development Assistant, provide visualization and intelligent technology to build Universal Application faster and better, support Web / H5 / MiniProgram(小程序) Application.",
  "publisher": "iceworks-team",
  "version": "0.12.0",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Extension Packs",
    "Programming Languages",
    "Visualization",
    "Debuggers",
    "Notebooks"
  ],
  "keywords": [
    "React",
    "Low Code",
    "Visual",
    "Intelli Code",
    "AI",
    "Autocomplete",
    "JavaScript",
    "TypeScript",
    "ICE",
    "Rax",
    "小程序"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ice-lab/iceworks.git"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "iceworks-ui-builder.design-component",
        "title": "%iceworksUIBuilder.commands.designComponent.title%"
      },
      {
        "command": "iceworks-project-creator.start",
        "title": "%iceworksProjectCreator.commands.start.title%"
      }
    ],
    "configuration": {
      "title": "Iceworks",
      "properties": {
        "iceworks.generateComponentPath": {
          "type": "string",
          "default": "",
          "description": "%iceworksUIBuilder.configuration.properties.iceworks.designComponentPath.description%"
        },
        "iceworks.materialSources": {
          "type": "array",
          "default": [],
          "description": "Iceworks materialSources"
        },
        "iceworks.workspace": {
          "type": "string",
          "default": "",
          "description": "Iceworks workspace"
        },
        "iceworks.user": {
          "type": "object",
          "default": {
            "empId": "",
            "account": "",
            "gitlabToken": ""
          },
          "properties": {
            "empId": {
              "type": "string",
              "description": "工号"
            },
            "account": {
              "type": "string",
              "description": "域账号"
            },
            "gitlabToken": {
              "type": "string",
              "description": "gitlab 私人 token"
            }
          },
          "description": "Iceworks user information"
        }
      }
    }
  },
  "icon": "assets/logo.png",
  "homepage": "https://github.com/ice-lab/iceworks/blob/master/extensions/iceworks/README.md",
  "bugs": {
    "url": "https://github.com/ice-lab/iceworks/issues",
    "email": "iceworksteam@163.com"
  },
  "dependencies": {
    "@ali/ide-extensions-iceworks-project-creator": "^0.3.17",
    "@ali/ide-extensions-iceworks-ui-builder": "^0.2.0",
    "@ali/kit-runner": "latest"
  },
  "devDependencies": {
    "@ali/kaitian": "latest",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.6",
    "typescript": "^3.4.5",
    "url-loader": "^4.1.0",
    "vscode": "^1.1.28",
    "webpack": "^4.43.0"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "node build.js"
  },
  "_moduleAliases": {
    "@project": "./out"
  },
  "kaitianContributes": {
    "nodeMain": "./lib/node/index.js"
  }
}
